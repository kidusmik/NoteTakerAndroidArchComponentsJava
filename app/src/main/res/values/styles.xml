<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CourseTitle" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:textColor">@color/colorCourseTitle</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="NoteTitle" parent="@style/TextAppearance.AppCompat.Body2">
        <item name="android:textColor">@color/colorNoteTitle</item>
        <item name="android:textStyle">bold</item>
    </style>



</resources>
